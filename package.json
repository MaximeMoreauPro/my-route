{
  "name": "my-route",
  "version": "0.0.1",
  "description": "My Route app and admin - Monorepo with web and mobile frontend and backend",
  "main": "dist/apps/cli.js",
  "scripts": {
    "build": "tsc",
    "code:format": "prettier src",
    "code:format-write": "prettier src --write",
    "code:lint": "eslint src --max-warnings 0",
    "code:typing": "tsc --noEmit",
    "test:unit": "jest",
    "test:mutation": "stryker run",
    "check-deps-update": "npx --yes npm-check-updates --reject @types/node"
  },
  "bin": {
    "my-route": "dist/apps/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaximeMoreauPro/my-route.git"
  },
  "author": "Maxime Moreau",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MaximeMoreauPro/my-route/issues"
  },
  "homepage": "https://github.com/MaximeMoreauPro/my-route#readme",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@stryker-mutator/core": "^7.1.0",
    "@stryker-mutator/jest-runner": "^7.1.0",
    "@types/jest": "^29.5.2",
    "@types/node": "20.5.4",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "babel-jest": "^29.5.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "commander": "^11.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "prettier-plugin-prisma": "^4.13.0",
    "prisma": "^4.16.1",
    "testcontainers": "^9.12.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "volta": {
    "node": "18.16.0",
    "yarn": "1.22.19"
  },
  "dependencies": {
    "@prisma/client": "^4.16.1",
    "uuid": "^9.0.0"
  }
}
