{
  "name": "my-route",
  "version": "0.0.1",
  "description": "My Route app and admin - Monorepo with web and mobile frontend and backend",
  "main": "dist/apps/cli.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "code:format": "prettier src",
    "code:format-write": "prettier src --write",
    "code:lint": "eslint src --max-warnings 0",
    "code:typing": "tsc --noEmit",
    "test:unit": "jest",
    "test:mutation": "stryker run",
    "check-deps-update": "npx --yes npm-check-updates --reject @types/node"
  },
  "bin": "dist/apps/cli.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaximeMoreauPro/my-route.git"
  },
  "author": "Maxime Moreau",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MaximeMoreauPro/my-route/issues"
  },
  "homepage": "https://github.com/MaximeMoreauPro/my-route#readme",
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-typescript": "^7.23.3",
    "@stryker-mutator/core": "^7.3.0",
    "@stryker-mutator/jest-runner": "^7.3.0",
    "@testcontainers/postgresql": "^10.3.2",
    "@types/jest": "^29.5.10",
    "@types/node": "20.10.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "commander": "^11.1.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^5.6.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.2"
  },
  "volta": {
    "node": "20.10.0",
    "yarn": "4.0.2"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "uuid": "^9.0.1"
  },
  "packageManager": "yarn@4.0.2"
}
